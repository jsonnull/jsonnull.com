    "build": "next build ./src && next export ./src -o ./public",
    "build:ci": "yarn build"
  },
  "dependencies": {
    "feather-icons": "^4.0.0",
    "gray-matter": "4.0.2",
    "markdown-to-jsx": "6.11.1",
    "next": "9.3.4",
    "puppeteer": "^1.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "tailwindcss": "1.1.4"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "prettier": "^1.6.1"
  }
}
