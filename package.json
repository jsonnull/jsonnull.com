{
  "name": "jsonnull.com",
  "private": true,
  "scripts": {
    "format": "prettier --write 'src/**/*.js' './*.js'",
    "screenshots": "node scripts/screenshots.js",
    "dev": "next ./src",
    "build": "tailwindcss -i src/global.css -o .css/bundle.css && ts-node scripts/screenshots && next build",
    "build:ci": "yarn build"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@popperjs/core": "^2.11.5",
    "chrome-aws-lambda": "^10.1.0",
    "clsx": "^1.1.1",
    "gray-matter": "4.0.3",
    "markdown-to-jsx": "7.1.7",
    "next": "latest",
    "puppeteer": "13.5.2",
    "puppeteer-core": "^13.7.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-feather": "^2.0.9",
    "react-helmet": "6.1.0",
    "react-popper": "^2.2.5"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.3.1",
    "@types/react": "^18.0.9",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
