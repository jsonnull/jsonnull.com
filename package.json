{
  "name": "jsonnull.com",
  "private": true,
  "scripts": {
    "format": "prettier --write 'src/**/*.js' './*.js'",
    "screenshots": "node scripts/screenshots.js",
    "dev": "next ./src",
    "build": "next build ./src && next export ./src -o ./public",
    "build:ci": "yarn build"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "gray-matter": "4.0.3",
    "markdown-to-jsx": "7.1.5",
    "next": "12.0.7",
    "puppeteer": "13.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "^2.0.9",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.1",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.8"
  }
}
