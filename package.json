    "recompose": "^0.26.0",
    "styled-components": "^2.2.1"
  },
  "devDependencies": {
    "app-root-path": "^2.0.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
