{
  "name": "jsonnull.com",
  "private": true,
  "scripts": {
    "format": "prettier --write 'src/**/*.js' './*.js'",
    "screenshots": "ts-node scripts/screenshots.tsx",
    "dev": "next ./src",
    "build": "tailwindcss -i src/global.css -o .css/bundle.css && ts-node scripts/screenshots && next build",
    "build:ci": "pnpm build"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@popperjs/core": "^2.11.5",
    "clsx": "^1.1.1",
    "gray-matter": "4.0.3",
    "markdown-to-jsx": "7.1.7",
    "next": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.9",
    "react-popper": "^2.2.5"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.3.1",
    "@resvg/resvg-js": "^2.1.0",
    "@types/node": "^18.11.4",
    "@types/react": "^18.0.9",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "prettier": "2.7.1",
    "satori": "^0.0.43",
    "tailwindcss": "3.2.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
